<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema path="/org/ubuntubudgie/plugins/budgie-wswitcher/"
            id="org.ubuntubudgie.plugins.budgie-wswitcher">
        <key type="b" name="runwswitcher">
            <summary>Run Wallpaper Switcher</summary>
            <description>Wether to run or pauze</description>
            <default>true</default>
        </key>
        <key type="as" name="wallpapers">
            <summary>Wallpaper per workspace (by index)</summary>
            <description>The list of wallpapers, ordered by workspace index</description>
            <default>["", "", ""]</default>
        </key>
    </schema>
</schemalist>


