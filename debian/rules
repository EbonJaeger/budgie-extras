#!/usr/bin/make -f
export DH_VERBOSE = 1
export DEB_BUILD_MAINT_OPTIONS = hardening=+all

%:
	dh $@ --with python3

override_dh_auto_configure:
	dh_auto_configure -- --prefix=/usr --libdir=/usr/lib --datadir=/usr/share -Dwith-zeitgeist=false

override_dh_python3:
	dh_python3 /usr/lib/budgie-desktop/plugins
