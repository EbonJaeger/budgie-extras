<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema path="/org/ubuntubudgie/plugins/budgie-dropby/"
            id="org.ubuntubudgie.plugins.budgie-dropby">
        <key type="i" name="popup-corner">
            <range min="1" max="4"/>
            <default>4</default>
            <summary>Corner where Dropby popup should appear</summary>
            <description>topleft = 1, topright = 2, bottomleft = 3, bottomright = 4</description>
        </key>
    </schema>
</schemalist>
